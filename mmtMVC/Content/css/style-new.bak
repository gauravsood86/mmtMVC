a.tooltip1 {
	position: relative;
	display: inline;
	text-decoration: none;
}
a.tooltip1 span {
	background: none repeat scroll 0 0 #D86363;
	border-radius: 6px;
	bottom: 36px;
	box-shadow: 2px 2px 0 #E3E3E3;
	color: #FFFFFF;
	font-size: 12px;
	height: 27px;
	left: -81%;
	line-height: 26px;
	margin-left: -76px;
	opacity: 1;
	position: absolute;
	text-align: center;
	visibility: visible;
	width: 123px;
	z-index: 999;
}
a.tooltip1 span:after {
	border-left: 8px solid rgba(0, 0, 0, 0);
	border-right: 8px solid rgba(0, 0, 0, 0);
	border-top: 8px solid #D86363;
	content: "";
	height: 0;
	left: 50%;
	margin-left: 32px;
	position: absolute;
	top: 100%;
	width: 0;
}


a.tooltipmas {
	position: relative;
	display: inline;
	text-decoration: none;
}
a.tooltipmas span {
	background: none repeat scroll 0 0 #D86363;
	border-radius: 6px;
	bottom: 36px;
	box-shadow: 2px 2px 0 #E3E3E3;
	color: #FFFFFF;
	font-size: 12px;
	height: 27px;
	left:-65%;
	line-height: 26px;
	margin-left: 48px;
	opacity: 1;
	position: absolute;
	text-align: center;
	visibility: visible;
	width: 123px;
	z-index: 999;
bottom:-5px;
}
a.tooltipmas span:after {
	border-left: 8px solid rgba(0, 0, 0, 0);
	border-right: 8px solid rgba(0, 0, 0, 0);
	border-top: 8px solid #D86363;
	content: "";
	height: 0;
	left: -50%;
	
	position: absolute;
	top: -47%;
	width: 0;
-ms-transform: rotate(90deg); /* IE 9 */
    -webkit-transform: rotate(90deg); /* Chrome, Safari, Opera */
    transform: rotate(90deg);
margin-left: 50px;
    margin-top: 22px;
}

